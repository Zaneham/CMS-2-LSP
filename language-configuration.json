{
  "comments": {
    "lineComment": "''",
    "blockComment": ["''", "''"]
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"]
  ],
  "autoClosingPairs": [
    {"open": "(", "close": ")"},
    {"open": "[", "close": "]"},
    {"open": "'", "close": "'"}
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(SYS-DD|SYS-PROC|SYS-PROC-REN|TABLE|TYPE|PROCEDURE|EXEC-PROC|FUNCTION|LOC-DD|AUTO-DD|BEGIN)\\b",
      "end": "^\\s*(END-SYS-DD|END-SYS-PROC|END-TABLE|END-TYPE|END-PROC|END-FUNCTION|END-LOC-DD|END-AUTO-DD|END)\\b"
    }
  },
  "wordPattern": "[A-Za-z][A-Za-z0-9_]*"
}
